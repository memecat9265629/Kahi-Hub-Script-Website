local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "Kahi Hub",
    LoadingTitle = "Kahi Hub - Loading",
    LoadingSubtitle = "By memecat9265629",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil, -- Change to save in a specific folder
        FileName = "KahiHubConfig"
    },
    KeySystem = true, -- Set to true for key system
    KeySettings = {
        Title = "Kahi Hub - Key System",
        Subtitle = "V0.1",
        Note = "Join Discord for the key: discord.gg/sabvPsPC",
        SaveKey = true,
        Key = "Kahi_Hub_UrMom"
    }
})

local Tab = Window:CreateTab("Main", 4483362458) -- Tab with a specific image ID

Tab:CreateButton({
    Name = "Click Me",
    Callback = function()
        print("Button clicked!")
    end,
})

Rayfield:Notify({
    Title = "Welcome!",
    Content = "Rayfield UI has been loaded successfully.",
    Duration = 5,
    Image = 4483362458
})
